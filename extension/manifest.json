{
  "manifest_version": 2,
  "name": "Safe Browser Extension",
  "version": "1.0.0",
  "description": "",
  "browser_action": {
    "default_title": "Safe Browser Extension",
    "default_popup": "index.html"
  },
  "author": "Gnosis",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "script.js"
  ],
  "content_security_policy": "script-src 'self' https://cdn.firebase.com https://*.firebaseio.com https://apis.google.com; object-src 'self'",
  "externally_connectable": {
    "ids": [],
    "matches": []
  }
}
